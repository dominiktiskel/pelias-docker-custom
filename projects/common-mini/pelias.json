{
  "logger": {
    "level": "info",
    "timestamp": false
  },
  "esclient": {
    "apiVersion": "7.5",
    "hosts": [
      {
        "host": "elasticsearch"
      }
    ]
  },
  "elasticsearch": {
    "settings": {
      "index": {
        "refresh_interval": "10s",
        "number_of_replicas": "0",
        "number_of_shards": "1"
      }
    }
  },
  "acceptance-tests": {
    "endpoints": {
      "docker": "http://api:4000/v1/"
    }
  },
  "api": {
    "services": {
      "libpostal": {
        "url": "http://libpostal:4400"
      }
    }
  },
  "imports": {
    "adminLookup": {
      "enabled": true
    },
    "whosonfirst": {
      "datapath": "/data/whosonfirst"
    },
    "openstreetmap": {
      "download": [
        {
          "sourceURL": "http://download.geofabrik.de/europe/poland-latest.osm.pbf"
        },
        {
          "sourceURL": "http://download.geofabrik.de/europe/great-britain-latest.osm.pbf"
        }
      ],
      "leveldbpath": "/tmp",
      "datapath": "/data/openstreetmap",
      "import": [
        {
          "filename": "poland-latest.osm.pbf"
        },
        {
          "filename": "great-britain-latest.osm.pbf"
        }
      ],
      "preferOsmAdmin": true,
      "useV2Pipeline": true,
      "aggregateHouseNumbers": true,
      "importStreets": true
    }
  }
}
